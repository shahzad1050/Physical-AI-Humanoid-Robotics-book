# Backend environment variables for Render/Railway deployment
# Copy this to your deployment platform's environment variables section

# OpenAI Configuration (if using OpenAI for embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# Cohere Configuration (if using Cohere for embeddings)
COHERE_API_KEY=your_cohere_api_key_here

# Qdrant Configuration (Vector Database)
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_key_if_needed

# Application Configuration
ENVIRONMENT=production
DEBUG=false
LOG_LEVEL=info

# CORS Configuration (already set in app.py, but can override here)
ALLOWED_ORIGINS=http://localhost:3000,https://physical-ai-humanoid-book-theta.vercel.app,https://shahzad1050.github.io

# Session Configuration
SESSION_TIMEOUT=3600
MAX_MESSAGES_PER_SESSION=100
